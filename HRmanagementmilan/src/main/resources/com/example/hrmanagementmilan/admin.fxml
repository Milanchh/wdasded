<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="378.0" prefWidth="621.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hrmanagementmilan.adminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="uid"/>
   <TextField fx:id="uname" />
   <TextField fx:id="uadress"/>
   <TextField fx:id="unum"/>
   <TableView fx:id="tableview" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="id" prefWidth="75.0" text="id" />
       <TableColumn fx:id="name" prefWidth="75.0" text="name" />
         <TableColumn fx:id="adress" prefWidth="75.0" text="adress" />
         <TableColumn fx:id="num" prefWidth="75.0" text="num" />
     </columns>
   </TableView>
   <Button fx:id="fetchbutton" mnemonicParsing="false" onAction="#fetchdata" text="fetch" />
   <Button fx:id="insertbutton" mnemonicParsing="false" onAction="#insertdata" text="insert" />
   <Button fx:id="deletebutton" onAction="#deletedata" mnemonicParsing="false" text="delete" />
   <Button fx:id="updatebutton" onAction="#updatedata" mnemonicParsing="false" text="update" />
   <Button fx:id="loadbutton" onAction="#loaddata" mnemonicParsing="false" text="load" />
    <Button mnemonicParsing="false" text="back" />

    <Label fx:id="welcomeText" />
</VBox>
